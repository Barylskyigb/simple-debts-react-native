{
  "extends": ["airbnb"],
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
    "react/require-default-props": 0,
    "react/forbid-prop-types": 0,
    "react/prefer-stateless-function": 0,
    "comma-dangle": ["error", "never"],
    "consistent-return": 0,
    "arrow-parens": 0,
    "no-underscore-dangle": 0,
    "default-case": 0
  }
}
